{
  "cli": {
    "version": ">= 16.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "ios": {
        "distribution": "store"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
            "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "juanjognzl@gmail.com",
        "appleTeamId": "S987NJW678",
        "companyName": "Controla Gestion SPA"
      }
    }
  }
}
