{
  "expo": {
    "name": "Gadve",
    "slug": "cadve2-1",
    "scheme": "cadve2.1",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/gadve-icon-3.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/gadve-icon-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#17355F"
    },
    "ios": {
      "supportsTablet": false,
      "buildNumber": "2",
      "bundleIdentifier": "com.juanjofrost.cadve21",
      "icon": "./assets/gadve-icon-3.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "GADVE necesita acceso a tu biblioteca de fotos para permitirte adjuntar imágenes como evidencia en los checklists de vehículos y reportes de daños. Por ejemplo, podrás seleccionar fotos del estado del vehículo, daños existentes, las fotografías requeridas para completar los procesos de entrega, seguimiento y devolución de vehículos arrendados."
      }
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "backgroundColor": "#17355F"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.juanjofrost.cadve21",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "55b140b4-a2f2-403d-a8cf-bb343c042ac3"
      }
    }
  }
}
